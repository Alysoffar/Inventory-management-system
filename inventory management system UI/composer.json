{
    "name": "alysoffar/smart-inventory-management",
    "type": "project",
    "description": "Advanced AI-Powered Inventory Management System with Laravel - Complete solution for inventory tracking, automated restocking, GPS location tracking, and predictive analytics",
    "keywords": [
        "laravel",
        "inventory",
        "management", 
        "ai-predictions",
        "gps-tracking",
        "automated-restocking",
        "real-time-monitoring",
        "warehouse-management",
        "supply-chain",
        "dashboard-analytics",
        "bootstrap",
        "responsive-ui"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alys Offar",
            "email": "alysoffar06@gmail.com",
            "homepage": "https://github.com/Alysoffar",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/Alysoffar/Inventory-management-system",
    "support": {
        "issues": "https://github.com/Alysoffar/Inventory-management-system/issues",
        "source": "https://github.com/Alysoffar/Inventory-management-system",
        "email": "alysoffar06@gmail.com"
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "guzzlehttp/guzzle": "^7.8",
        
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "symfony/postmark-mailer": "^7.0",
        
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        
        "predis/predis": "^2.0",
        
        "laravel/horizon": "^5.21",
        "laravel/telescope": "^5.0",
        
        "barryvdh/laravel-dompdf": "^2.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-medialibrary": "^11.0",
        
        "intervention/image": "^3.0",
        
        "pusher/pusher-php-server": "^7.0",
        
        "laravel/scout": "^10.0",
        "algolia/algoliasearch-client-php": "^3.0",
        
        "doctrine/dbal": "^3.0",
        
        "nesbot/carbon": "^2.68",
        
        "ramsey/uuid": "^4.7",
        
        "league/csv": "^9.0",
        
        "phpoffice/phpspreadsheet": "^1.29"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "laravel/breeze": "^2.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4",
        "spatie/laravel-ignition": "^2.3",
        
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^2.13",
        
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.0",
        
        "phpmd/phpmd": "^2.15",
        
        "rector/rector": "^0.18",
        
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan storage:link --ansi"
        ],
        "fresh": [
            "@php artisan migrate:fresh --seed",
            "@php artisan optimize:clear"
        ],
        "setup": [
            "@composer install",
            "@php artisan key:generate",
            "@php artisan migrate --seed",
            "@php artisan storage:link",
            "@php artisan optimize"
        ],
        "test": [
            "@php artisan test"
        ],
        "test-coverage": [
            "@php artisan test --coverage"
        ],
        "analyze": [
            "@php ./vendor/bin/phpstan analyse",
            "@php ./vendor/bin/pint --test"
        ],
        "format": [
            "@php ./vendor/bin/pint"
        ],
        "security": [
            "@composer audit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/Alysoffar"
        }
    ]
}
