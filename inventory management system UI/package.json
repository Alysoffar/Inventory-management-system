{
    "name": "smart-inventory-management-system",
    "version": "2.0.0",
    "description": "Advanced AI-Powered Inventory Management System with Laravel - Real-time tracking, automated restocking, GPS location mapping, and predictive analytics dashboard",
    "keywords": [
        "laravel",
        "inventory-management",
        "ai-predictions",
        "gps-tracking",
        "automated-restocking",
        "bootstrap",
        "leaflet",
        "real-time-monitoring",
        "dashboard-analytics",
        "responsive-ui",
        "warehouse-management",
        "supply-chain",
        "chart-js",
        "modern-ui"
    ],
    "author": {
        "name": "Alys Offar",
        "email": "alysoffar06@gmail.com",
        "url": "https://github.com/Alysoffar"
    },
    "license": "MIT",
    "homepage": "https://github.com/Alysoffar/Inventory-management-system",
    "repository": {
        "type": "git",
        "url": "https://github.com/Alysoffar/Inventory-management-system.git"
    },
    "bugs": {
        "url": "https://github.com/Alysoffar/Inventory-management-system/issues",
        "email": "alysoffar06@gmail.com"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "main": "resources/js/app.js",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "watch": "vite build --watch",
        "hot": "vite --host",
        "preview": "vite preview",
        "production": "cross-env NODE_ENV=production npm run build",
        "lint": "eslint resources/js --ext .js,.vue",
        "lint:fix": "eslint resources/js --ext .js,.vue --fix",
        "format": "prettier --write \"resources/**/*.{js,vue,css,scss}\"",
        "format:check": "prettier --check \"resources/**/*.{js,vue,css,scss}\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "analyze": "npm run lint && npm run format:check",
        "clean": "rimraf public/build node_modules/.cache",
        "reinstall": "npm run clean && npm install"
    },
    "devDependencies": {
        "@vitejs/plugin-laravel": "^0.7.8",
        "laravel-vite-plugin": "^0.8.1",
        "vite": "^4.5.0",
        
        "axios": "^1.6.0",
        "lodash": "^4.17.21",
        
        "bootstrap": "^5.3.2",
        "@popperjs/core": "^2.11.8",
        "sass": "^1.69.5",
        "postcss": "^8.4.31",
        "autoprefixer": "^10.4.16",
        
        "cross-env": "^7.0.3",
        
        "eslint": "^8.54.0",
        "prettier": "^3.1.0",
        
        "jest": "^29.7.0",
        "@babel/preset-env": "^7.23.5",
        "babel-jest": "^29.7.0",
        
        "rimraf": "^5.0.5",
        
        "@fortawesome/fontawesome-free": "^6.5.0"
    },
    "dependencies": {
        "leaflet": "^1.9.4",
        "chart.js": "^4.4.0",
        "chartjs-adapter-date-fns": "^3.0.0",
        "date-fns": "^2.30.0",
        
        "sweetalert2": "^11.10.1",
        
        "select2": "^4.1.0-rc.0",
        "bootstrap-select": "^1.14.0-beta3",
        
        "jquery": "^3.7.1",
        "jquery-ui": "^1.13.2",
        
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.43",
        
        "datatables.net": "^1.13.7",
        "datatables.net-bs5": "^1.13.7",
        "datatables.net-responsive": "^2.5.0",
        "datatables.net-responsive-bs5": "^2.5.0",
        
        "toastr": "^2.1.4",
        
        "aos": "^2.3.4",
        
        "prismjs": "^1.29.0",
        
        "quill": "^1.3.7",
        
        "sortablejs": "^1.15.1",
        
        "dropzone": "^6.0.0-beta.2",
        
        "fullcalendar": "^6.1.10",
        
        "apexcharts": "^3.45.1"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "<rootDir>/tests/javascript/setup.js"
        ],
        "moduleNameMapping": {
            "^@/(.*)$": "<rootDir>/resources/js/$1"
        }
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es2021": true,
            "jquery": true
        },
        "extends": [
            "eslint:recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "globals": {
            "Laravel": "readonly",
            "route": "readonly",
            "Chart": "readonly",
            "L": "readonly",
            "Swal": "readonly"
        }
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "avoid"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/Alysoffar"
        }
    ]
}
