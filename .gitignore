# ===================================
# INVENTORY MANAGEMENT SYSTEM .GITIGNORE
# ===================================
# Optimized for Laravel + AI/ML hybrid project
# Only excludes files NOT required for running the system

# ===================================
# ENVIRONMENT & CONFIGURATION FILES
# ===================================
# These contain sensitive data and should be configured locally
.env
.env.local
.env.production
.env.staging
*.env

# Keep example files for setup guidance
!.env.example

# ===================================
# DEPENDENCY DIRECTORIES
# ===================================
# These are large and can be regenerated from lock files

# Python dependencies
.venv/
venv/
__pycache__/
*.py[cod]
*$py.class
*.egg-info/

# Laravel PHP dependencies
inventory\ management\ system\ UI/vendor/

# Node.js dependencies
inventory\ management\ system\ UI/node_modules/
inventory\ management\ system\ UI/npm-debug.log*
inventory\ management\ system\ UI/yarn-error.log

# ===================================
# GENERATED/COMPILED FILES
# ===================================
# These are built from source and can be regenerated

# Laravel compiled assets
inventory\ management\ system\ UI/public/hot
inventory\ management\ system\ UI/public/storage
inventory\ management\ system\ UI/public/mix-manifest.json

# Python compiled files
*.pyc
*.pyo
*.pyd
build/
dist/

# ===================================
# RUNTIME DATA & LOGS
# ===================================
# Generated during runtime, not needed in repository

# Laravel logs and cache
inventory\ management\ system\ UI/storage/logs/*.log
inventory\ management\ system\ UI/storage/framework/cache/data/*
inventory\ management\ system\ UI/storage/framework/sessions/*
inventory\ management\ system\ UI/storage/framework/views/*

# Keep the directory structure
!inventory\ management\ system\ UI/storage/logs/.gitkeep
!inventory\ management\ system\ UI/storage/framework/cache/data/.gitkeep
!inventory\ management\ system\ UI/storage/framework/sessions/.gitkeep
!inventory\ management\ system\ UI/storage/framework/views/.gitkeep

# Python logs
*.log

# ===================================
# USER DATA & UPLOADS
# ===================================
# User-generated content that should not be in git

# Laravel user uploads (keep public directories)
inventory\ management\ system\ UI/storage/app/public/uploads/*
!inventory\ management\ system\ UI/storage/app/public/uploads/.gitkeep

# ===================================
# SENSITIVE BUSINESS DATA
# ===================================
# Never commit actual business data

# Large datasets (use sample data instead)
data/retail_store_inventory.csv
data/*.csv
data/*.xlsx
data/*.json

# Training results containing business metrics
results/
training_results.json
performance_metrics.json

# Diagnostic images (may contain business data)
diogonistic\ images/
*.png
*.jpg
*.jpeg
*.gif
!docs/images/*.png
!docs/images/*.jpg

# ===================================
# LARGE MODEL FILES
# ===================================
# AI models are large and should use Git LFS or cloud storage

models/*.h5
models/*.pkl
models/*.joblib
*.h5
*.pkl
*.joblib
*.model
*.weights

# Keep model architecture files and small config files
!models/config.json
!models/README.md

# ===================================
# DEVELOPMENT & TESTING
# ===================================
# Development artifacts not needed for production

# IDE files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# OS files
Thumbs.db
.DS_Store
.directory

# Temporary files
*.tmp
*.temp
*.bak
*.swp
*~

# Test coverage
htmlcov/
.coverage
coverage.xml

# ===================================
# KEEP ESSENTIAL FILES
# ===================================
# Force include important files that might be caught by patterns above

# Laravel essential files
!inventory\ management\ system\ UI/artisan
!inventory\ management\ system\ UI/composer.json
!inventory\ management\ system\ UI/composer.lock
!inventory\ management\ system\ UI/package.json
!inventory\ management\ system\ UI/webpack.mix.js

# Python essential files
!requirements.txt
!setup.py

# Documentation
!README.md
!INTEGRATION_GUIDE.md
!AI_INTEGRATION_SETUP.md

# Sample/Example files
!Sample_PHP_Connection.php
!data/sample_inventory.csv.example

# Configuration templates
!config_template.py

# ===================================
# DEPLOYMENT ARTIFACTS
# ===================================
# Don't commit deployment-specific files

docker-compose.override.yml
.docker/
Dockerfile.prod

# ===================================
# BACKUP FILES
# ===================================
# Backup files should not be in version control

*.backup
backup/
*.zip
*.tar.gz
*.rar
